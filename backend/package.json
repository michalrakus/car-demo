{
  "name": "car-demo-backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "linkLib": "pnpm link @michalrakus/x-nest-server-lib",
    "buildAndPackLib": "pnpm --prefix ../../XNestServerLib run buildAndPack",
    "installLib": "pnpm install ../../XNestServerLib/michalrakus-x-nest-server-lib-1.34.0.tgz -w",
    "bl": "pnpm run buildAndPackLib && pnpm run installLib",
    "sl": "pnpm run bl && pnpm run start",
    "preinstall": "npx only-allow pnpm"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@michalrakus/x-nest-server-lib": "1.34.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/typeorm": "^10.0.2",
    "angular-expressions": "^1.1.10",
    "docxtemplater": "^3.42.7",
    "exceljs": "^4.4.0",
    "jwks-rsa": "^3.0.1",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pg-query-stream": "^4.5.5",
    "pizzip": "^3.1.4",
    "postgres-interval": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "ts-parse-database-url": "^1.0.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/jest": "29.4.0",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "29.4.3",
    "prettier": "^2.8.3",
    "supertest": "^6.3.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
